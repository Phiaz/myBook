  <div class="container mt-4">
  {{#each reviews}}
    <div class="card mt-4" style="width: 35rem; margin-left: 200px">
            <ul class="list-group list-group-flush">
        <a href="/public/{{this.userId}}"><li class="list-group-item">{{this.userName}}</li></a>
      </ul>
      <img src="{{this.img}}" style="" class="card-img-top" alt="...">
      <div class="card-body">
        <h5 class="card-title">{{this.bookName}}</h5>
        <p class="card-text">Description</p>
      </div>
      <ul class="list-group list-group-flush">
        <li class="list-group-item">{{this.author}}</li>
      </ul>
      <div class="card-body">
        <a href="#" class="card-link">Xem bài viết</a>
        <a href="#" class="card-link">Xem trang cá nhân</a>
      </div>
    </div>
    {{/each}}

<form action="">
  <nav class="mt-4" style= "margin-bottom: 10px; margin-left: 200px " aria-label="Page navigation example">
      <ul class="pagination">
        {{#each pagination}}
        <input class="page-item btn btn-primary" type ="submit" class="page-link" value="{{this}}" name="page">
        {{/each}}
      </ul>
    </nav>
</form>
  </div>




